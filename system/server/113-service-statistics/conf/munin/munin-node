[apache_request_mapserver]
user root

[cpu_by_process]
env.procs nbd-server postgres

[df*]
env.exclude none unknown iso9660 squashfs udf romfs ramfs debugfs
env.warning 92
env.critical 98

[multips_memory]
env.names nbd-server postgres

[nginx_request_webgis]
user root

[postgres_*]
user postgres
env.PGUSER postgres
env.PGPORT 5432
