# Install GIS.lab graphic

case "$MODE" in
    finalization)
	cp -a $GISLAB_INSTALL_CLIENT_ROOT/ldm/themes/gislab $ROOT/usr/share/ldm/themes/
	cp $GISLAB_INSTALL_CLIENT_ROOT/wallpaper/gislab-wallpaper.png $ROOT/usr/share/xfce4/backdrops/gislab-wallpaper.png
	cp /var/www/gislab-web/static/viewer/images/gislab-web.xpm $ROOT/usr/share/pixmaps/gislab-web.xpm
    ;;
esac
