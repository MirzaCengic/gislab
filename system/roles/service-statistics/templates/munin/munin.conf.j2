contact.admins.command mail -s "[GIS.lab ID: {{ GISLAB_UNIQUE_ID }}] ${var:graph_title}" root

[gis.lab;]

[gis.lab;server]
	address 127.0.0.1
	use_node_name yes

{% if GISLAB_PROVIDER == 'aws' %}
[gis.lab;c3.gis.lab]
	address {{ GISLAB_NETWORK }}.3
	use_node_name yes

[gis.lab;c4.gis.lab]
	address {{ GISLAB_NETWORK }}.4
	use_node_name yes

{% for n in range(6,50) %}
[gis.lab;c{{ n }}.gis.lab]
	address {{ GISLAB_NETWORK }}.{{ n }}
	use_node_name yes

{% endfor %}
{% endif %}
{% for n in range(50,150) %}
[gis.lab;c{{ n }}.gis.lab]
	address {{ GISLAB_NETWORK }}.{{ n }}
	use_node_name yes

{% endfor %}
{% if GISLAB_PROVIDER == 'aws' %}
{% for n in range(150,255) %}
[gis.lab;c{{ n }}.gis.lab]
	address {{ GISLAB_NETWORK }}.{{ n }}
	use_node_name yes

{% endfor %}
{% endif %}
