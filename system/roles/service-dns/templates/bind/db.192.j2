$TTL    604800
$ORIGIN {{ GISLAB_NETWORK_SERVER_IP|reverse_ip }}.IN-ADDR.ARPA.
@  3600  IN    SOA       ns.gis.lab. root.gis.lab. (
               2013112103     ; Serial
               604800         ; Refresh
               86400          ; Retry
               2419200        ; Expire
               604800         ; Negative Cache TTL
               )

         NS              ns.gis.lab.

; server
5        IN    PTR       server.gis.lab.

; clients
3  IN  PTR  c3.gis.lab.
4  IN  PTR  c4.gis.lab.
{% for ip in range(6,255) %}
{{ ip }}  IN  PTR  c{{ ip }}.gis.lab.
{% endfor %}
